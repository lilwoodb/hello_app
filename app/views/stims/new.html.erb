<% provide(:title, 'Submit') %>
<h1>Submit</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@stim) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :description %>
      <%= f.text_field :description %>

      <%= f.label :source %>
      <%= f.text_field :source %>

      <%= f.submit "Add your stim!", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
